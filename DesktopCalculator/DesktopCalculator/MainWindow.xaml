<Window x:Class="DesktopCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopCalculator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="num1" HorizontalAlignment="Left" Height="23" Margin="193,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="result" Content="Label" HorizontalAlignment="Left" Margin="220,138,0,0" VerticalAlignment="Top"/>
        <Button x:Name="executeButton" Content="Button" HorizontalAlignment="Left" Margin="193,220,0,0" VerticalAlignment="Top" Width="75" Click="executeButton_Click"/>

    </Grid>
</Window>
